{
    "plugins": [
        "theme-faq",
        "disqus",
        "search",
        "lunr",
        "fontsettings",
        "sharing"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "jaredmorgs",
            "useIdentifier": false
        },
        "lunr": {
            "maxIndexSize": "1000000"
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": "2"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": true,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": "facebook,google,twitter,weibo,instapaper"
        }
    }
}