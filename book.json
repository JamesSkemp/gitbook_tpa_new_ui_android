{
    "plugins": [
        "theme-faq",
        "fontsettings",
        "sharing",
        "lunr"
        "search"
    ],
    "structure": {
        "readme": "home.adoc"
    },
    "pluginsConfig": {
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": "2"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": true,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": "facebook,google,twitter,weibo,instapaper"
        },
        "lunr": {
            "maxIndexSize": "1000000"
        }
    }
}